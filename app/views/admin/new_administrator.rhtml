<% if @found %>
   <h1>Edit Administrator</h1>
 <% else %>
   <h1>New Administrator</h1>
 <% end %>
<%= flash[:notice] %>
<%= start_form_tag :action => 'create_administrator' %>
  <%= render :partial => 'm' %>
  <% if @found %>
   <%= submit_tag "Save", :name=>"save" %>
 <% else %>
   <%= submit_tag "Create" %>
 <% end %>
<%= end_form_tag %>

<br />
<%= link_to 'Back', :action => 'list_administrators' %>