<% if @teammate_review_scores.size > 0 %>
<table class=listing cellpadding =2>
    <tr>
        <th>Questions</th><th>Comments</th><th>Score</th>
    </tr>
    <% for prs in @teammate_review_scores %>
        <tr>
            <td>
                <%=prs.question.txt%>                
            </td>
            <td>
                <%=prs.comments%>
            </td>
            <td><%=prs.score%></td>
        </tr>
    <% end %>
</table>
<% end%>