<h1>Edit Teammate Review</h1>
<h2>Teammate: <%= User.find(@teammate_review.reviewee_id).name %></h2>
<%= start_form_tag :action => 'update' %>
<%= render :partial => 'teammate_review_content' %>    
<%= hidden_field_tag('teammate_review_id', @teammate_review.id) %>
